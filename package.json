{
  "name": "Moonman",
  "version": "0.3.0",
  "author": "Shing Rui <sunquakes@outlook.com>",
  "description": "A cross-platform tcp desktop software. Create servers and clients.ðŸš€",
  "keywords": [
    "electron",
    "vue",
    "sqlite3",
    "tcp client",
    "tcp server"
  ],
  "license": "MIT License",
  "main": "main.js",
  "build": {
    "productName": "Moonman",
    "appId": "io.moonquakes.moonman",
    "asar": true,
    "directories": {
      "output": "build"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "test": "jest",
    "dev": "node .electron-kits/dev-runner.js",
    "build": "vite build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "electron-store": "^8.1.0",
    "element-ui": "^2.15.14",
    "font-awesome": "^4.7.0",
    "moment": "^2.29.4",
    "sqlite3": "^5.1.6",
    "vue": "^2",
    "vue-i18n": "^8.27.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@vitejs/plugin-vue2": "^2.3.1",
    "aws-sdk": "^2.1473.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.1",
    "electron": "^26.3.0",
    "electron-builder": "^24.6.4",
    "jest": "^29.7.0",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.3.4",
    "vite": "^4.4.5",
    "vue-template-compiler": "^2.7.16"
  }
}
